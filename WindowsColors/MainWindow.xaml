<Window x:Name="mainWindow" x:Class="WindowsColors.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowsColors"
        mc:Ignorable="d"
        Title="Windows Colors" Height="780" Width="600" Loaded="Window_Loaded">
    <Grid Name="mainGrid">

        <StackPanel x:Name="stackPanel1" Margin="0,30,302,0"/>
        <StackPanel x:Name="stackPanel2" Margin="302,30,0,0"/>
        <Label x:Name="label1" Content="HKEY_CURRENT_USER\Control Panel\Colors" HorizontalAlignment="Left" VerticalAlignment="Top" Width="243"/>
        <Label x:Name="label2" Content="HKEY_CURRENT_USER\Control Panel\Desktop\Colors" HorizontalAlignment="Left" VerticalAlignment="Top" Width="289" Margin="302,0,0,0"/>

    </Grid>
</Window>
